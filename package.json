{
  "name": "doc.linea",
  "version": "3.3.1",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "prebuild": "node scripts/generateSocialCard.js && node scripts/updateCategories.js",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "clean-slate": "bash ./clean-slate.sh",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "typecheck-staged": "tsc-files --noEmit",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:style": "stylelint \"**/*.css\"",
    "lint:style:fix": "npm run lint:style -- --fix",
    "format": "prettier --write \"{blog,docs,src,static}/(**/*).{md,mdx,ts,js,tsx,jsx,json}\""
  },
  "lint-staged": {
    "src/**/*.{ts,js,jsx,tsx}": "npm run lint:js:fix",
    "**/*.{ts,tsx}": "npm run typecheck-staged",
    "**/*.css": "npm run lint:style",
    "**/*.{md,mdx,ts,js,tsx,jsx,json}": "npm run format"
  },
  "dependencies": {
    "@docusaurus/core": "^3.9.2",
    "@docusaurus/faster": "^3.9.2",
    "@docusaurus/plugin-client-redirects": "^3.9.2",
    "@docusaurus/plugin-content-docs": "^3.9.2",
    "@docusaurus/plugin-google-gtag": "^3.9.2",
    "@docusaurus/plugin-google-tag-manager": "^3.9.2",
    "@docusaurus/preset-classic": "^3.9.2",
    "@docusaurus/theme-mermaid": "^3.9.2",
    "@easyops-cn/docusaurus-search-local": "^0.51.1",
    "@mdx-js/react": "^3.1.1",
    "@mermaid-js/layout-elk": "^0.1.9",
    "axios": "^1.13.2",
    "browserslist": "^4.28.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "dompurify": "^3.3.0",
    "dotenv": "^16.6.1",
    "gray-matter": "^4.0.3",
    "hast-util-is-element": "^3.0.0",
    "ignore": "^5.3.2",
    "postcss-safe-parser": "^7.0.1",
    "prism-react-renderer": "^1.3.5",
    "raw-loader": "^4.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-player": "^2.16.1",
    "redocusaurus": "^2.5.0",
    "sharp": "^0.33.5",
    "typescript-eslint": "^8.48.1",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@docusaurus/module-type-aliases": "^3.9.2",
    "@docusaurus/tsconfig": "^3.9.2",
    "@eslint/compat": "^1.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.3.5",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/dompurify": "^3.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^15.15.0",
    "lint-staged": "^15.5.2",
    "prettier": "3.3.3",
    "semantic-release": "^24.2.9",
    "stylelint": "16.13.0",
    "stylelint-config-standard": "35.0.0",
    "tsc-files": "^1.1.4",
    "typescript": "^5.9.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "last 2 versions",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^22"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
